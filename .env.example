# Environment Variables for Netlify/Railway Deployment
# Copy this file to .env.local for local development

# Google Gemini AI API Key (required)
API_KEY=your_google_gemini_api_key_here

# Telegram Bot Token (required for bot functionality)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Database URL (optional - for Railway/production database)
DATABASE_URL=postgresql://username:password@host:port/database

# Node Environment
NODE_ENV=production

# Host and Port (usually not needed for Netlify static deployment)
HOST=0.0.0.0
PORT=8080